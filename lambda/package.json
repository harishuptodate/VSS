{
	"name": "video-thumb-lambda",
	"version": "1.0.0",
	"type": "commonjs",
	"main": "dist/handler.js",
	"scripts": {
		"build": "esbuild handler.ts --bundle --platform=node --target=node20 --outfile=dist/handler.js",
		"zip": "powershell -Command \"Compress-Archive -Path 'dist/handler.js' -DestinationPath 'function.zip' -Force\"",
		"package": "npm run build && npm run zip"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.45.0",
		"execa": "^8.0.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.129",
		"@types/node": "^20.12.12",
		"esbuild": "^0.23.0"
	}
}
